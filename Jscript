		function myFunction() {

  		var node = document.createElement("LI");
 		 	var textnode = document.createElement("Input");	
 				 node.appendChild(textnode);
				 
  	document.getElementById("myList").appendChild(node);  
  	var myNodelist = document.querySelectorAll("input");

var i;
	for (i = 0; i < myNodelist.length; i++) {
  		myNodelist[i].style.backgroundColor = "#69f";
			}
  		} 
		
function myFunctionz() {
 var c = document.getElementById("myList").childElementCount;

  	document.getElementById("demo").innerHTML = c;
var cless = c -1 ;
 for (q=0; q<cless; q++){
     	
        var zero = 0;
        var x = parseFloat(document.getElementById("myList").childNodes.nodeValue);
        	document.getElementById("yeee").innerHTML = (zero += x[q]);
     }

 for (i=0; i<=cless; i++) {

 var z = parseFloat(document.getElementById("myList").childNodes.nodeValue);
 var txt = "";

 txt += "The node\'s value is: " + z[i] + "<br>";
 
 	 document.getElementById("yee").innerHTML = txt;

	}

}

